    </custom-attribute-definitions>
    <group-definitions>
      <attribute-group group-id="Bolt Attributes">
        <display-name xml:lang="x-default">Bolt Attributes</display-name>
        <attribute attribute-id="boltTransactionReference"/>
      </attribute-group>
    </group-definitions>
